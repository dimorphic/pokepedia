{
  "name": "pokepedia",
  "version": "0.0.1",
  "description": "Pokepedia Club",
  "author": "Sabin Tudor <contact@sabin-tudor.ro>",
  "contributors": [],
  "license": "TBA",
  "repository": "dimorphic/pokepedia",
  "homepage": "https://github.com/dimorphic/pokepedia#readme",
  "bugs": {
    "url": "https://github.com/dimorphic/pokepedia/issues"
  },
  "keywords": [
    "pokemon"
  ],
  "engines": {
    "node": ">= 6.0.0"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon --exec './node_modules/.bin/babel-node src/'",
    "start:prod": "cross-env NODE_ENV=production ./node_modules/.bin/nodemon --quiet --watch build --exec 'node build/'",
    "dev": "npm run clean && ./node_modules/.bin/babel src --out-dir build --watch --debug",
    "build": "npm run clean && cross-env NODE_ENV=production ./node_modules/.bin/babel src --out-dir build --source-maps inline",
    "clean": "rm -rf ./build",
    "lint": "./node_modules/.bin/eslint src --ext .js"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "cross-env": "^2.0.0",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "jsonfile": "^2.3.1",
    "morgan": "^1.7.0",
    "nodemon": "^1.10.0",
    "pokemon-go-pokedex": "git+https://github.com/Biuni/PokemonGO-Pokedex.git",
    "request": "^2.74.0",
    "tracer": "^0.8.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-node6": "^0.2.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-import-resolver-node": "^0.2.2",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0"
  }
}
